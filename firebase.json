{
  "hosting": {
    "public": "dist",
    "site": "vision-ai-host",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://cdn.jsdelivr.net/"
          }
        ]
      }
    ]
  }
}
